# apt-mirror configuration file

##
## The default configuration options (uncomment and change to override)
##
#
set base_path	   /var/www/apt
# set mirror_path  $base_path/mirror
# set skel_path	   $base_path/skel
# set var_path     $base_path/var
#
# set defaultarch  <running host architecture>
set nthreads     5
#
set _tilde 0 

# Etch
deb http://ftp.uni-kl.de/debian/ etch main contrib non-free main/debian-installer
deb-src http://ftp.uni-kl.de/debian/ etch main contrib non-free

deb http://security.debian.org/ etch/updates main contrib non-free
deb-src http://security.debian.org/ etch/updates main contrib non-free

deb ftp://debian.netcologne.de/debian-multimedia.org etch main
deb-src ftp://debian.netcologne.de/debian-multimedia.org etch main

# Lenny
deb http://ftp.uni-kl.de/debian/ lenny main contrib non-free main/debian-installer
deb-src http://ftp.uni-kl.de/debian/ lenny main contrib non-free

deb http://security.debian.org/ lenny/updates main contrib non-free
deb-src http://security.debian.org/ lenny/updates main contrib non-free

deb ftp://debian.netcologne.de/debian-multimedia.org lenny main
deb-src ftp://debian.netcologne.de/debian-multimedia.org lenny main


