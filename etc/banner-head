#!/bin/bash
black="\033[0;30m"
dark_gray="\033[1;30m"
blue="\033[0;34m"
light_blue="\033[1;34m"
green="\033[0;32m"
light_green="\033[1;32m"
cyan="\033[0;36m"
light_cyan="\033[1;36m"
red="\033[0;31m"
light_red="\033[1;31m"
purple="\033[0;35m"
light_purple="\033[1;35m"
brown="\033[0;33m"
yellow="\033[1;33m"
light_gray="\033[0;37m"
white="\033[1;37m"
none="\033[0m"

g=$green 
r=$red
w=$light_gray

echo -ne $g'                            ___====-_  _-====___\n'
echo -ne '                      _--~~~#####//      \\\\#####~~~--_\n'
echo -ne '                   _-~##########// (    ) \\\\##########~-_\n'
echo -ne '                  -############//  :\^^/:  \\\\############-\n'
echo -ne '                _~############//   ('$r'@'$g'::'$r'@'$g')   \\\\############~_\n'
echo -ne '               ~#############((     \\\\//     ))#############~\n'
echo -ne '              -###############\\\\    (oo)    //###############-\n'
echo -ne '             -#################\\\\  / '$w'""'$g$' \  //#################-\n'
echo -ne '            -###################\\\\/      \//###################-\n'
echo -ne '           _#/:##########/\######(   /\   )######/\##########:\#_\n'
echo -ne '           :/ :#/\#/\#/\/  \#/\##\  :  :  /##/\#/  \/\#/\#/\#: \:\n'
echo -ne '           '$w'"'$g'  :/  V  V  '$w'"'$g$'   V  \#\: :  : :/#/  V   '$w'"'$g$'  V  V  \:  '$w'"'$g'\n'
echo -ne '              '$w'"'$g'   '$w'"'$g'  '$w'"'$g'      '$w'"'$g'   / : :  : : \   '$w'"'$g'      '$w'"'$g'  '$w'"'$g'   '$w'"'$g'\n'
echo -ne '                               (  : :  : :  )\n'
echo -ne '                              __\ : :  : : /__\n'
echo -ne '                             ('$w'vvv'$g'('$w'VVV'$g')('$w'VVV'$g')'$w'vvv'$g')\n'$w
