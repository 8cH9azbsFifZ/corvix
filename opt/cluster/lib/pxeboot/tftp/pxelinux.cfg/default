DISPLAY boot.txt

#LABEL linux
#	KERNEL kernel-default
#	APPEND server=10.1.1.1 dev=eth0 initrd=initrd-default
#	INITRD initrd-default
#	IPAPPEND 1

LABEL install
        kernel debian/etch/i386/linux
	append vga=normal initrd=debian/etch/i386/initrd.gz  --

LABEL rescue
	kernel debian/etch/i386/linux
	append vga=normal initrd=debian/etch/i386/initrd.gz  rescue/enable=true --

LABEL makenode
	kernel makenode/kernel
	append vga=normal initrd=makenode/initrd.gz server=10.1.1.1  nocomm dontwait targethd=/dev/sda --

PROMPT 1
TIMEOUT=0
